{
    "configurations": [
        {
            "name": "Linux",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "compilerPath": "${env:HOME}/.espressif/tools/xtensa-esp32-elf/esp-2021r2-patch5-8.4.0/xtensa-esp32-elf/bin/xtensa-esp32-elf-gcc",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "includePath": [
                "${env:HOME}/esp/esp-idf-v4.4.4/components/**",
                "${env:HOME}/esp/esp-adf-v2.5-org/components/**",
                "${workspaceFolder}/components/**"
            ],
            "browse": {
                "path": [
                    "${env:HOME}/esp/esp-idf-v4.4.4/components/**",
                    "${env:HOME}/esp/esp-adf-v2.5-org/components/**"
                    ],
                "limitSymbolsToIncludedHeaders": false
            },
            "defines": [
                "ESP_PLATFORM=1"
            ]
        }
    ],
    "version": 4
}